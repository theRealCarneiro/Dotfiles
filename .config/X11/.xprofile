#! /bin/sh 
xrdb $XDG_CONFIG_HOME/X11/.Xresources # Source Xresources
bluetoothctl scan on > /dev/null &
setbg & # Wallpaper
xss-lock -- screenlock &
xset r rate 300 50 &
xset s on &
xset s 900 &
picom & # Compositor
dunstLaunch & # Notification
#polyLaunch &
sxhkdl &
dbusfm &
#randbg &
lockscreen &
