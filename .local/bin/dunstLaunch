#!/bin/sh 
. /home/gabriel/.cache/wal/shell
killall dunst 2> /dev/null
dunst -lf  "${main}" \
      -lb  "${background}" \
      -lfr "${background}" \
      -nf  "${main}" \
      -nb  "${background}" \
      -nfr "${background}" \
      -cf  "${main}" \
      -cb  "${background}" \
      -cfr "${main}" 
